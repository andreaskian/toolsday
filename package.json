{
  "name": "toolsday",
  "version": "1.0.0",
  "description": "A 20-minute weekly podcast hosted by Una Kravets and Chris Dhanaraj about tech tooling!",
  "main": "gulpfile.js",
  "scripts": {
    "serve": "harp server _harp",
    "build": "harp compile _harp --output ../www",
    "deploy": "harp compile _harp --output ../www && gh-pages -d www",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "express": "^4.14.0",
    "harp": "0.20.3"
  },
  "devDependencies": {
    "browser-sync": "2.6.4",
    "gh-pages": "0.5.0",
    "gulp": "3.8.11",
    "node-sass": "3.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/una/toolsday"
  },
  "keywords": [
    "podcast",
    "front-end",
